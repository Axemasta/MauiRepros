<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="Prism" value="https://nuget.prismlibrary.com/v3/index.json" protocolVersion="3" />
  </packageSources>
  <packageSourceMapping>
    <clear />
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="Prism">
      <package pattern="Prism.Core" />
      <package pattern="Prism.DryIoc.Maui" />
      <package pattern="Prism.Events" />
      <package pattern="Prism.Maui" />
      <package pattern="Prism.Maui.Rx" />
    </packageSource>
  </packageSourceMapping>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
</configuration>
